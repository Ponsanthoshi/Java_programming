import java.util.Arrays;
import java.util.Collections;
import java.util.Scanner;

public class KthAndLargestElements {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        Integer[] arr = new Integer[n]; // Must be Integer[] for reverse sort
        for (int i = 0; i < n; i++) {
            arr[i] = scanner.nextInt();
        }
        int k = scanner.nextInt();
        Arrays.sort(arr);
        System.out.print("The elements are in the order: ");
        for (int i = 0; i < n; i++) {
            System.out.print(arr[i]);
            if (i < n - 1) {
                System.out.print(" ");
            }
        }
        System.out.println();
        System.out.print("The Kth value is " + k + " and Largest elements are ");
        Arrays.sort(arr, Collections.reverseOrder());
        for (int i = 0; i < k && i < arr.length; i++) {
            System.out.print(arr[i]);
            if (i < k - 1 && i < arr.length - 1) {
                System.out.print(" ");
            }
        }

        System.out.println();
        scanner.close();
    }
}

