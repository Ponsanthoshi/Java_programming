import java.util.Scanner;

public class SquareSum {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int sum = 0;
        for (int i = 0; i < n; i++) {
            int number = scanner.nextInt();
            if (number > 0) {
                sum += number * number; 
            }
        }
        System.out.println(sum);
        scanner.close();
    }
}
