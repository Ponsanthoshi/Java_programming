import java.util.HashSet;
import java.util.Scanner;

public class DistinctElementsCounter {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = scanner.nextInt();
        }

        HashSet<Integer> uniqueElements = new HashSet<Integer>();

        for (int i = 0; i < n; i++) {
            uniqueElements.add(arr[i]);
        }

        int count = uniqueElements.size();
        if (count == 1) {
            System.out.println("There are 1 distinct element in the array.");
        } else {
            System.out.println("There are " + count + " distinct elements in the array.");
        }

        scanner.close();
    }
}
